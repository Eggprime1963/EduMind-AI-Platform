{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build and Deploy Learning Platform",
			"type": "shell",
			"command": "mvn",
			"args": [
				"clean",
				"package"
			],
			"group": "build",
			"problemMatcher": [
				"$tsc"
			],
			"isBackground": false
		},
		{
			"label": "Maven Clean Compile",
			"type": "shell",
			"command": "mvn clean compile",
			"group": "build"
		},
		{
			"label": "Compile Java Project",
			"type": "shell",
			"command": "mvn",
			"args": [
				"clean",
				"compile"
			],
			"isBackground": false,
			"group": "build"
		}
	]
}